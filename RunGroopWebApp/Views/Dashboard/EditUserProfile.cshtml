@model RunGroopWebApp.ViewModels.EditUserDashboardViewModel
@{
}
<form method="post" asp-action="EditUserProfile" enctype="multipart/form-data">
    <input type="hidden" id="Id" name="Id" asp-for="Id"/>
    <div class="form-group">
        <label asp-for="Pace">Pace</label>
        <input asp-for="Pace" class="form-control" placeholder="Pace">
        <span class="text-danger" asp-validation-for="Pace"></span>
    </div>
    <div class="form-group">
        <label asp-for="Mileage">Mileage</label>
        <input asp-for="Mileage" class="form-control" placeholder="Mileage">
        <span class="text-danger" asp-validation-for="Mileage"></span>
    </div>
    <div class="form-group">
        <label asp-for="City">City</label>
        <input asp-for="City" class="form-control" placeholder="City">
        <span class="text-danger" asp-validation-for="City"></span>
    </div>
    <div class="form-group">
        <label asp-for="State">State</label>
        <input asp-for="State" class="form-control" placeholder="State">
        <span class="text-danger" asp-validation-for="State"></span>
    </div>
    <div class="form-group">
        <label asp-for="Image">Image</label>
        <input asp-for="Image" class="form-control" placeholder="Image">
        <span class="text-danger" asp-validation-for="Image"></span>
    </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>